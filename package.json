{
  "name": "qr-code-with-logo",
  "version": "1.1.0",
  "description": "qrCode with logo (image), 无jQuery依赖, 自由调整大小",
  "repository": {
    "type": "git",
    "url": "https://github.com/HerbLuo/qr-code-with-logo.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "unpkg": "lib/index.umd.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "dist:dev": "rollup --watch -c ./rollup.config.js",
    "dist": "rollup -c ./rollup.config.js",
    "test:dev": "webpack-dev-server --config test/module/webpack.dev.conf.js --mode development",
    "publish2npm": "npm run dist && npm publish"
  },
  "keywords": [
    "qr-code",
    "logo",
    "image",
    "qrCode",
    "size"
  ],
  "author": "herbluo",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.2.6",
    "qrcode": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "p-single": "^1.0.9",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
